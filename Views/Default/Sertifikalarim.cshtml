@using Mvc5.Models.Entity
@model List<tbl_sertifikalarim>

<div class="resume-section-content">
    <h2 class="mb-5">Aldığım Eğitimler</h2>
    <ul class="fa-ul mb-0">
        @foreach (var x in Model)
        {
            <li>
                <span class="fa-li"><i class="fas fa-trophy text-warning"></i></span>
                @x.Aciklama
            </li>
        }
    </ul>
</div>
